<?xml version="1.0" encoding="iso-8859-1"?>
<!--
  This sample shows what a request for movies
  in the bioguiden system can look like.
-->
<document schema="RepertoireDistributorExportSchema1_0.xsd">
    	<!--
        	The values in the fields contained by this
        	"information" element is to be set by the
        	requesting system. The values are treated as
        	strings by the Bioguiden system except for
        	the "created" field that has to be a valid
        	date and time. The values are not used for
        	other than logging and back tracking purpose.
    	-->
    	<information>
    		<name>Folkets bio import</name>
    		<description>Retrieve screenings from the Bioguiden database at https://service.bioguiden.se</description>
    		<log-id>c3f23168-7f3a-44fd-9bfd-28e0735d6c27</log-id>
    		<created>CREATEDDATE</created>
    		<server>Folkets bio</server>
    		<ip>127.0.0.1</ip>
    	</information>
    	<data>
	    	<!--
	      	It is possible to get repertoire only for certain movies.
	      	If the "movies" tag exists repertoire will be returned
	      	only for the given movies (if the calling user has
	      	access to these of course).  If the "movies" tag is missing
	      	then repertoire for all movies that the calling user has
	      	access to will be returned.
	    	-->
    		<movies>
      			<movie />
    		</movies>

    		<!--
      		Retrieves repertoire between the given start and end date and time.
    		-->
    		<start-date>STARTDATE</start-date>
    		<end-date>ENDDATE</end-date>
  	</data>
</document>